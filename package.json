  "author": "Josh Aguilar <j.serafin.aguilar@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "minimist": "^1.2.0",
    "selenium-webdriver": "^3.4.0"
  },
  "devDependencies": {
